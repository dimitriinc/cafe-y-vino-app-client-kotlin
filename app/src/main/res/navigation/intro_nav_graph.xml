<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/intro_nav_graph"
    app:startDestination="@id/walkthroughFragment">

    <fragment
        android:id="@+id/bienvenidoFragment"
        android:name="com.cafeyvinowinebar.cafe_y_vino_client.ui.introduction.BienvenidoFragment"
        android:label="fragment_bienvenido"
        tools:layout="@layout/fragment_bienvenido" >
        <action
            android:id="@+id/action_bienvenidoFragment_to_registrationFragment"
            app:destination="@id/registrationFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"/>
        <action
            android:id="@+id/action_bienvenidoFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/walkthroughFragment"
        android:name="com.cafeyvinowinebar.cafe_y_vino_client.ui.introduction.onboarding.OnBoardingFragment"
        android:label="fragment_walkthrough"
        tools:layout="@layout/fragment_on_boarding" >
        <action
            android:id="@+id/action_walkthroughFragment_to_bienvenidoFragment"
            app:destination="@id/bienvenidoFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/registrationFragment"
        android:name="com.cafeyvinowinebar.cafe_y_vino_client.ui.introduction.RegistrationFragment"
        android:label="fragment_registration"
        tools:layout="@layout/fragment_registration" >
        <action
            android:id="@+id/action_registrationFragment_to_privacyFragment"
            app:destination="@id/privacyFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"/>
        <action
            android:id="@+id/action_registrationFragment_to_mainActivity"
            app:destination="@id/mainActivity"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainActivity"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.cafeyvinowinebar.cafe_y_vino_client.ui.introduction.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_mainActivity"
            app:destination="@id/mainActivity"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainActivity"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/privacyFragment"
        android:name="com.cafeyvinowinebar.cafe_y_vino_client.ui.introduction.PrivacyFragment"
        android:label="fragment_privacy_policy"
        tools:layout="@layout/fragment_privacy_policy" />
    <activity
        android:id="@+id/mainActivity"
        android:name="com.cafeyvinowinebar.cafe_y_vino_client.ui.main.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>